{
	"name": "rpdvn-portfolio",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"analyze": "ANALYZE=true next build",
		"dev": "next dev --turbo",
		"build": "next lint --quiet && next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,json,md,ts,tsx}": [
			"pretty-quick --staged",
			"eslint --fix"
		]
	},
	"dependencies": {
		"next": "14.2.3",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^14.2.3",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.5",
		"prettier": "^3.2.5",
		"pretty-quick": "^4.0.0",
		"typescript": "^5"
	}
}
